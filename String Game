int main() {
	long long int t;
	cin>>t;
	while(t--){
	    int n,o=0,z=0;
	    cin>>n;
	    string s;
	    cin>>s;
	    int c=0;
	    for(int i=0;i<n;i++){
	        if(s[i]=='1'){
	            o++;
	        }
	        else(s[i]=='0'){
	            z++;
	        }
	    }
	    int pairs=min(o,z);
	    if(pairs%2==0){
	        cout<<"Ramos"<<endl;
	    }
	    else{
	        cout<<"Zlatan"<<endl
	    }
	    
	}
	return 0;
    
}
